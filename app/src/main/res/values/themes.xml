        <!-- Primary brand color. -->
        <item name="colorPrimary">#00CBA9</item>
        <item name="colorPrimaryVariant">#62FFD9</item>
        <item name="colorOnPrimary">#2B00CBA9</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#342500</item>
        <item name="colorSecondaryVariant">#E4A400</item>
        <item name="colorOnSecondary">#FFB800</item>
