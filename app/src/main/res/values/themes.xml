<resources>
    <!-- Base application theme. -->
    <style name="Theme.Aechatsdk" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/Primary40</item>
        <item name="colorPrimaryVariant">@color/Primary40</item>
        <item name="colorOnPrimary">@color/Primary40</item>
        <item name="android:navigationBarColor">@color/transparent</item>

        <!-- Optional: set to transparent if your app is drawing behind the status bar. -->
        <item name="android:statusBarColor">@color/transparent</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/Primary40</item>
        <item name="colorSecondaryVariant">@color/Primary40</item>
        <item name="colorOnSecondary">@color/Primary40</item>
        <!-- Customize your theme here. -->
        <item name="colorAccent">@color/Primary10</item>
    </style>

    <style name="Theme.Aechatsdk.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Aechatsdk.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Aechatsdk.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>