<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Aechatsdk" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#2F2E41</item>
        <item name="colorPrimaryVariant">#50C2C9</item>
        <item name="colorOnPrimary">#FFFFFF</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#2F2E41</item>
        <item name="colorSecondaryVariant">#50C2C9</item>
        <item name="colorOnSecondary">#FFFFFF</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Aechatsdk.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Aechatsdk.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Aechatsdk.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
